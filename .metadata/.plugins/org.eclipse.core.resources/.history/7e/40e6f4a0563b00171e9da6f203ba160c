package sorting;
class sel{
	void sele(int[] x){
	for(int i=0;i<x.length;i++){
		for(int j=1;j<x.length;j++){
			if(x[j]<x[i]){
				int y=x[j];
				int k=x[i];
				x[i]=y;
				x[j]=k;
				
			}
			else{
				continue;
			}
		}
	}
System.out.println(x);		
	}
}
public class selec {
	public static void main(String[] args){
		int[] m=new int[]{1,5,9,4,3,2};
		sel s=new sel();
		s.sele(m);
	}
	

}
