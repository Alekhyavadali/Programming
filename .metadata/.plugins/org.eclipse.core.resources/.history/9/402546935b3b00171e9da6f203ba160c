package sorting;
class bub{
	void bubb(int[] x){
		for(int k=0;k<x.length;k++){
		for(int i=0,j=1;i<x.length;i++,j++){
			if(x[j]<x[i]){
				int y=x[i];
				int m=x[j];
				x[j]=y;
				x[i]=m;
			}else{
				continue;
			}
		}
		}for(int i=x.length-1;i>=0;i--){	
			System.out.println(x[i]);		
		}
	}
}
public class bubble {

	public static void main(String[] args) {
int[] m=new int[]{45,23,87,12,9};
			bub s=new bub();
			s.bubb(m);
	}

}
