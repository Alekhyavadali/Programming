package sorting;
class sel{
	void sele(int[] x){
	
		for(int j=1,i=0;j<x.length;j++,i++){
			if(x[j]<x[i]){
				int y=x[j];
				int k=x[i];
				x[i]=y;
				x[j]=k;
				
			}
			else{
				continue;
			}
		}
	
	for(int i=0;i<x.length;i++){	
System.out.println(x[i]);		
	}}
}
public class selec {
	public static void main(String[] args){
		int[] m=new int[]{1,5,9,4,3,2};
		sel s=new sel();
		s.sele(m);
	}
	

}
