package sorting;
class ins{
	void ins(int[] x){
		for(int i=1;i<x.length;i++){
			for(int k=i-1;k>=0;k--){
			if(x[i]<x[k]){
			int	val=x[i];
			int y=x[k];
			x[k]=val;
			x[i]=y;
			}
			}
		}
		for(int i=0;i<x.length;i++){	
			System.out.println(x[i]);
	}
}}
public class insertion {

	public static void main(String[] args) {
	ins m=new ins();
	int[] h=new int[]{7,1,6,2,5,4};
	m.ins(h);

	}

}
