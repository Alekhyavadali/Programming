package sorting;
class bub{
	void bubb(int[] x){
		for(int k=0;k<x.length;k++){
		for(int i=0;i<x.length;i++){
			if(x[i+1]<x[i]){
				int y=x[i];
				int m=x[i+1];
				x[i+1]=y;
				x[i]=m;
			}else{
				continue;
			}
		}
		}for(int i=x.length-1;i>=0;i--){	
			System.out.println(x[i]);		
		}
	}
}
public class bubble {

	public static void main(String[] args) {
int[] m=new int[]{45,23,87,12,9};
			bub s=new bub();
			s.bubb(m);
	}

}
